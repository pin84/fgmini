.df {
  display: flex;
  align-items: center;
  justify-content: center;
}

#home {
  box-sizing: border-box;
  height: 100vh;
  padding: 40rpx;
}

#home .list {
  display: flex;
  flex-wrap: wrap;
}

#home .list .item {
  padding: 10rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: center;
  width: 140rpx;
  height: 140rpx;
  border: 1px solid #ccc;
  position: relative;
  border-radius: 10rpx;
  margin: 5rpx;
}

#home .list .item .close {
  position: absolute;
  right: 0;
  top: 0;
  padding: 5rpx;
  color: #ccc;
}

#home .list .item .uploadIcon {
  font-size: 96rpx;
  color: #ccc;
  font-weight: 100;
}

#home .list .item .image {
  width: 100%;
  height: 100%;
}

#home .btn {
  margin-top: 40rpx;
  width: 80vw;
}

#home .text {
  color: #ccc;
  margin-top: 50rpx;
  font-size: 24rpx;
}
